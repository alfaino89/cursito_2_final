{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-cursillobp"
		},
		"Ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'Ls_databricks'"
		},
		"ls_dsdatafactory_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_dsdatafactory'"
		},
		"ls_sqlserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqlserver'"
		},
		"ls_kvcursodatafactory_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kvdatafactorycurso.vault.azure.net/"
		},
		"SC_MINUTE_PAR_IMPAR_properties_buclesito_parameters_numerin": {
			"type": "int",
			"defaultValue": 12
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Ls_databricks_accessToken": {
						"value": "[parameters('Ls_databricks_accessToken')]"
					},
					"ls_dsdatafactory_connectionString": {
						"value": "[parameters('ls_dsdatafactory_connectionString')]"
					},
					"ls_sqlserver_connectionString": {
						"value": "[parameters('ls_sqlserver_connectionString')]"
					},
					"ls_kvcursodatafactory_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kvcursodatafactory_properties_typeProperties_baseUrl')]"
					},
					"SC_MINUTE_PAR_IMPAR_properties_buclesito_parameters_numerin": {
						"value": "[parameters('SC_MINUTE_PAR_IMPAR_properties_buclesito_parameters_numerin')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}